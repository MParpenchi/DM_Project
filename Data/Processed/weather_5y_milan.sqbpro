x="8" value="53"/><column index="9" value="73"/><column index="10" value="71"/><column index="11" value="80"/><column index="12" value="129"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="final_weather_accidents_filtered" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="37"/><column index="2" value="42"/><column index="3" value="43"/><column index="4" value="176"/><column index="5" value="65"/><column index="6" value="42"/><column index="7" value="43"/><column index="8" value="53"/><column index="9" value="110"/><column index="10" value="71"/><column index="11" value="80"/><column index="12" value="129"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">DROP TABLE IF EXISTS final_weather_accidents_filtered;

CREATE TABLE final_weather_accidents_filtered AS
SELECT *
FROM final_weather_accidents
WHERE Anno &gt;= 2020
ORDER BY Anno, Mese;

DROP TABLE IF EXISTS weather_monthly;

CREATE TABLE weather_monthly AS
SELECT
    substr(date, 1, 7) AS month,
    AVG(tavg) AS avg_temp,
    SUM(prcp) AS total_rain,
    SUM(snow) AS total_snow,
    SUM(CASE WHEN prcp &gt; 0 THEN 1 ELSE 0 END) AS rain_days,
    SUM(CASE WHEN snow &gt; 0 THEN 1 ELSE 0 END) AS snow_days
FROM daily_weather_point
GROUP BY substr(date, 1, 7)
ORDER BY month;

DROP TABLE IF EXISTS final_weather_accidents_filtered;

CREATE TABLE final_weather_accidents_filtered AS
SELECT
    a.*,
    w.avg_temp,
    w.total_rain,
    w.total_snow,
    w.rain_days,
    w.snow_days,
    CASE
        WHEN w.snow_days &gt; 0 THEN 'snow'
        WHEN w.rain_days &gt; 0 THEN 'rain'
        ELSE 'dry'
    END AS weather_condition
FROM accidents_monthly a
LEFT JOIN weather_monthly w ON a.month = w.month
WHERE Anno &gt;= 2020
ORDER BY a.month;

SELECT weather_condition,
       SUM(Incidenti) AS total_accidents
FROM final_weather_accidents_filtered
GROUP BY weather_condition;

DROP TABLE IF EXISTS final_weather_accidents_filtered;

CREATE TABLE final_weather_accidents_filtered AS
SELECT
    a.*,
    w.avg_temp,
    w.total_rain,
    w.total_snow,
    w.rain_days,
    w.snow_days,
    CASE
        WHEN w.snow_days &gt;= 2 THEN 'snow'
        WHEN w.total_rain &gt;= 15 OR w.rain_days &gt;= 5 THEN 'rain'
        ELSE 'dry'
    END AS weather_condition
FROM accidents_monthly a
LEFT JOIN weather_monthly w ON a.month = w.month
WHERE Anno &gt;= 2020
ORDER BY a.month;

SELECT weather_condition, COUNT(*) 
FROM final_weather_accidents_filtered
GROUP BY weather_condition;

DROP TABLE IF EXISTS final_weather_accidents_filtered;

CREATE TABLE final_weather_accidents_filtered AS
SELECT
    a.*,
    w.avg_temp,
    w.total_rain,
    w.total_snow,
    w.rain_days,
    w.snow_days,
    CASE
        WHEN w.snow_days &gt;= 1 THEN 'snow'
        WHEN w.total_rain &gt; 10 OR w.rain_days &gt;= 3 THEN 'rain'
        ELSE 'dry'
    END AS weather_condition
FROM accidents_monthly a
LEFT JOIN weather_monthly w ON a.month = w.month
WHERE Anno &gt;= 2020
ORDER BY a.month;

SELECT weather_condition, COUNT(*)
FROM final_weather_accidents_filtered
GROUP BY weather_condition;

SELECT
    month,
    SUM(Incidenti) AS monthly_accidents,
    AVG(avg_temp) AS monthly_temp,
    AVG(total_rain) AS monthly_rain
FROM final_weather_accidents_filtered
GROUP BY month
ORDER BY month;

SELECT
    CASE
        WHEN total_rain &gt;= 50 THEN 'high_rain'
        WHEN total_rain BETWEEN 10 AND 50 THEN 'medium_rain'
        ELSE 'low_rain'
    END AS rain_level,
    AVG(Incidenti) AS avg_accidents
FROM final_weather_accidents_filtered
GROUP BY rain_level;

SELECT 
    CASE 
        WHEN total_rain &lt; 10 THEN 'Low Rain'
        WHEN total_rain BETWEEN 10 AND 40 THEN 'Medium Rain'
        ELSE 'High Rain'
    END AS Rain_Level,
    AVG(Incidenti) AS avg_accidents,
    AVG(Feriti) AS avg_injuries
FROM final_weather_accidents_filtered
GROUP BY Rain_Level;

SELECT 
    CASE WHEN snow_days &gt; 0 THEN 'Snowy' ELSE 'Non-snow' END as SnowImpact,
    AVG(Morti) AS avg_deaths,
    AVG(Feriti) AS avg_injuries,
    AVG(Incidenti) AS avg_accidents
FROM final_weather_accidents_filtered
GROUP BY SnowImpact;

SELECT 
    month,
    avg_temp,
    total_rain,
    SUM(Incidenti) as accidents
FROM final_weather_accidents_filtered
GROUP BY month
ORDER BY month;


